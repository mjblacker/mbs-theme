{
  "name": "cearls/timberland",
  "type": "wordpress-theme",
  "description": "A modern WordPress theme using Timber, ACF, Tailwind CSS and Alpine JS",
  "license": "MIT",
  "authors": [{
    "name": "Chris Earls",
    "email": "cgearls@gmail.com"
  }],
  "require": {
    "php": ">= 8.2",
    "timber/timber": "^2.2"
  },
  "require-dev": {
    "aspireweb/sapling-composer-paths-plugin": "dev-main",
    "johnpbloch/wordpress-core": "^6.8.2",
    "johnpbloch/wordpress-core-installer": "^2.0"
  },
  "extra": {
      "wordpress-install-dir": {
          "johnpbloch/wordpress-core": "../wp"
      }
  },
  "autoload": {
      "psr-4": {
          "Sapling\\": "app/Sapling",
          "App\\": "app/"
      },
      "classmap": ["app/Sapling/Integrations/", "app/Sapling/Sapling.php"]
  },
  "repositories": [
    {
      "type": "path",
      "url": "sapling-composer-paths-plugin"
    }
  ],
  "config": {
    "vendor-dir": "vendor",
    "optimize-autoloader": false,
    "sort-packages": true,
    "allow-plugins": {
      "aspireweb/sapling-composer-paths-plugin": true,
      "johnpbloch/wordpress-core-installer": true
    }
  },
  "minimum-stability": "stable"
}
