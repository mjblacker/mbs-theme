{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "840px",
			"wideSize": "1100px"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "Lexend, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "Lexend",
					"slug": "lexend",
					"fontFace": [
						{
							"fontFamily": "Lexend",
							"fontWeight": "100",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/Lexend/Lexend-Thin.ttf"
							]
						},
						{
							"fontFamily": "Lexend",
							"fontWeight": "200",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/Lexend/Lexend-ExtraLight.ttf"
							]
						},
						{
							"fontFamily": "Lexend",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/Lexend/Lexend-Light.ttf"
							]
						},
						{
							"fontFamily": "Lexend",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/Lexend/Lexend-Regular.ttf"
							]
						},
						{
							"fontFamily": "Lexend",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/Lexend/Lexend-Medium.ttf"
							]
						},
						{
							"fontFamily": "Lexend",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/Lexend/Lexend-SemiBold.ttf"
							]
						},
						{
							"fontFamily": "Lexend",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/Lexend/Lexend-Bold.ttf"
							]
						},
						{
							"fontFamily": "Lexend",
							"fontWeight": "800",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/Lexend/Lexend-ExtraBold.ttf"
							]
						},
						{
							"fontFamily": "Lexend",
							"fontWeight": "900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/Lexend/Lexend-Black.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"size": "13px",
					"slug": "small"
				},
				{
					"size": "20px",
					"slug": "medium"
				},
				{
					"size": "36px",
					"slug": "large"
				},
				{
					"size": "42px",
					"slug": "x-large"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"lineHeight": true,
			"textDecoration": true,
			"textTransform": true
		},
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotones": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#f8f9fa",
					"name": "Light Gray",
					"slug": "light-gray"
				},
				{
					"color": "#6c757d",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "#343a40",
					"name": "Dark Gray",
					"slug": "dark-gray"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--lexend)",
			"lineHeight": "1.6"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--dark-gray)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lexend)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lexend)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lexend)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lexend)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lexend)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lexend)"
				}
			}
		}
	}
}